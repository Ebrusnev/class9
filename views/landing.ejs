<body>
	 <nav class="navbar navbar-inverse">
      <div class="container">
        <div id="navbar" class="navbar-collapse collapse">
        </div><!--/.navbar-collapse -->
      </div>
    </nav>

    <div class="container">
      <!-- Example row of columns -->
      <div class="row">
      		<% articles.forEach(function(artcl) {%>	        
	      		<div class="col-xs-12 col-md-4">
		          <h2><%=artcl.name%></h2>
		          <p><%=artcl.body%></p>
		          <p><a class="btn btn-default" href="/<%=artcl.id%>" role="button">View details &raquo;</a></p>
		        </div> 			
      		<% }); %>
	      </div>
      <hr>
      <footer>
        <p>&copy; 2017 Company, Inc.</p>
      </footer>
    </div> <!-- /container -->
</body>
</html>